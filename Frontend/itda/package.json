    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/interaction": "^4.4.0",
    "@fullcalendar/vue": "^4.4.0",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "firebase": "^7.8.1",
    "vue": "^2.6.10",
    "vue-carousel": "^0.18.0",
    "vue-file-upload": "^0.1.12",
    "vue-infinite-loading": "^2.4.4",
    "vue-js-modal": "^1.3.31",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.1.3",
